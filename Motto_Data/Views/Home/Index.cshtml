@{
    ViewData["Title"] = "Home Page";
    string[] TableHeaders = new string[] {"Address ID"
                        ,"Address Line"
                        ,"City"
                        ,"State Province ID"
                        ,"Postal Code"
                        ,"Spatial Location"
                        ,"Row ID"
                        ,"Modified Date"};
}
<style>
    #topLayer {
        display: flex;
        justify-content: space-between;
        margin: 25px;
    }

    #min {
        margin-bottom: 8px;
    }

    #inpDate {
        font-weight: bold;
        padding-top: 5px;
        padding-right: 2px;
    }
</style>

<table class="table">
    <tbody>
        <tr>
            <td id="inpDate">Minimum date:</td>
            <td><input class="input is-rounded is-dark" type="text" id="min" name="min" /></td>
        </tr>
        <tr>
            <td id="inpDate">Maximum date:</td>
            <td><input class="input is-rounded is-dark" type="text" id="max" name="max" /></td>
        </tr>
    </tbody>
</table>
<table class="table is-bordered is-striped is-narrow is-hoverable is-fullwidth" id="example" style="width: 100%">
    <thead>
        <tr class="has-background-black-ter">
            @{
                foreach (var head in TableHeaders)
                {
                    <th class="has-text-white">
                        @head
                    </th>
                }
            }
        </tr>
    </thead>

    <tbody>
        @{
            if (Model != null)
            {
                foreach (var Data in Model)
                {
                    <tr>
                        <td>@Data.AddressID</td>
                        <td>@Data.AddressLine</td>
                        <td>@Data.City</td>
                        <td>@Data.StateProvinceID</td>
                        <td>@Data.PostalCode</td>
                        <td>@Data.SpatialLocation</td>
                        <td>@Data.RowID</td>
                        <td>@Data.ModifiedDate</td>
                    </tr>
                }
            }
        }
    </tbody>
</table>


<input type="text" name="datetimes" />

